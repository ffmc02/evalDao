<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.gaetan.gui.ClientControler">
   <children>
      <TableView fx:id="ListeClient" layoutX="31.0" layoutY="23.0" onMouseClicked="#SetForm" prefHeight="202.0" prefWidth="221.0" AnchorPane.leftAnchor="31.0" AnchorPane.rightAnchor="548.0">
        <columns>
           <TableColumn fx:id="id" prefWidth="75.0" text="ID" visible="false" />
          <TableColumn fx:id="Name" prefWidth="75.0" text="Nom" />
          <TableColumn fx:id="Firstname" prefWidth="75.0" text="Prenom" />
            <TableColumn fx:id="City" prefWidth="85.0" text="Ville" />
        </columns>
      </TableView>
      <Button fx:id="updateClient" disable="true" layoutX="483.0" layoutY="133.0" mnemonicParsing="false" onAction="#updateFormClient" text="Modifier" visible="false" />
      <Button fx:id="AddClitent" layoutX="486.0" layoutY="23.0" mnemonicParsing="false" onAction="#AddformCLient" text="Ajouter" />
      <Button fx:id="deleteClient" disable="true" layoutX="477.0" layoutY="213.0" mnemonicParsing="false" onAction="#DeleteformClient" text="Supprimer" visible="false" />
      <VBox fx:id="FormUser" alignment="TOP_CENTER" disable="true" layoutX="587.0" layoutY="77.0" prefHeight="345.0" prefWidth="186.0" visible="false">
         <children>
            <Label alignment="BOTTOM_LEFT" text="Nom" textAlignment="CENTER" />
            <TextField fx:id="NameUser">
               <VBox.margin>
                  <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
               </VBox.margin>
            </TextField>
            <Label alignment="BOTTOM_LEFT" text="Prenom" textAlignment="CENTER" />
            <TextField fx:id="FirstnameUser" />
            <Label fx:id="CityV" alignment="BOTTOM_LEFT" text="Ville" textAlignment="CENTER" />
            <TextField fx:id="CityUser" />
            <HBox alignment="TOP_CENTER" prefHeight="116.0" prefWidth="186.0">
               <children>
                  <Button fx:id="Validate" mnemonicParsing="false" onAction="#ValidateForm" text="Valider" />
                  <Button fx:id="Return" mnemonicParsing="false" onAction="#hideFormClient" text="Annuler " />
               </children>
            </HBox>
         </children>
      </VBox>
      <Label fx:id="TitleForm" layoutX="666.0" layoutY="15.0" />
   </children>
</AnchorPane>
